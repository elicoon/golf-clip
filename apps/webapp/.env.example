# apps/webapp/.env.example
# PostgreSQL connection string
GOLFCLIP_DATABASE_URL=postgresql://user:password@host:5432/golfclip

# Cloudflare R2 credentials
GOLFCLIP_R2_ACCOUNT_ID=your_account_id
GOLFCLIP_R2_ACCESS_KEY_ID=your_access_key
GOLFCLIP_R2_SECRET_ACCESS_KEY=your_secret_key
GOLFCLIP_R2_BUCKET=golfclip-videos

# CORS origins (comma-separated for multiple)
GOLFCLIP_CORS_ORIGINS=["https://your-app.vercel.app","http://localhost:5173"]

# Optional
GOLFCLIP_DEBUG=false
GOLFCLIP_MAX_VIDEO_SIZE_MB=500
