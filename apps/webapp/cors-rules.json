{
  "rules": [
    {
      "allowed": {
        "methods": ["GET", "PUT", "HEAD"],
        "origins": [
          "http://localhost:5173",
          "http://localhost:3000",
          "https://golfclip.vercel.app"
        ],
        "headers": ["content-type", "content-length"]
      },
      "exposeHeaders": ["ETag", "Content-Length"],
      "maxAgeSeconds": 3600
    }
  ]
}
